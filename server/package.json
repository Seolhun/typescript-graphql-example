{
  "name": "graphql-example",
  "version": "1.0.0",
  "description": "Graphql example",
  "main": "app.ts",
  "scripts": {
    "all": "concurrently \"npm run client\" \"npm run server\"",
    "client": "cd ../client && nodemon build/dev-server.js",
    "server": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "seolhun",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-session": "^1.15.6",
    "graphql": "^0.12.3",
    "helmet": "^3.12.0",
    "jest": "^22.4.2",
    "lodash": "^4.17.5",
    "mysql2": "^1.5.2",
    "nodemon": "^1.17.2",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-github2": "^0.1.11",
    "passport-google-oauth20": "^1.0.0",
    "sequelize": "^4.36.1",
    "sinon": "^4.4.8",
    "winston": "^3.0.0-rc2"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/bluebird": "^3.5.20",
    "@types/express": "^4.11.1",
    "@types/express-graphql": "0.0.35",
    "@types/graphql": "^0.11.8",
    "@types/helmet": "0.0.37",
    "@types/jest": "^22.2.0",
    "@types/lodash": "^4.14.104",
    "@types/passport": "^0.4.3",
    "@types/sequelize": "^4.27.10",
    "@types/sinon": "^4.3.0",
    "ts-jest": "^22.4.2",
    "typescript": "^2.7.2"
  }
}
