{
  "name": "graphql-example",
  "version": "1.0.0",
  "description": "Graphql example",
  "main": "app.ts",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/app.ts",
    "build": "tsc",
    "start": "node dist/app.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "seolhun",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "cookie-session": "^2.0.0-beta.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-graphql": "^0.7.1",
    "express-session": "^1.15.6",
    "graphql": "^14.2.0",
    "helmet": "^3.12.0",
    "jest": "^24.5.0",
    "lodash": "^4.17.5",
    "mariadb": "^2.1.0",
    "nodemon": "^2.0.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-github2": "^0.1.11",
    "passport-google-oauth20": "^2.0.0",
    "sequelize": "^5.2.3",
    "sinon": "^7.3.1",
    "winston": "^3.0.0-rc2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.11.1",
    "@types/express-graphql": "0.8.1",
    "@types/graphql": "^14.0.7",
    "@types/helmet": "0.0.44",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.148",
    "@types/passport": "^1.0.0",
    "@types/sequelize": "^4.28.4",
    "@types/sinon": "^7.0.10",
    "dotenv": "^8.1.0",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.2"
  }
}
